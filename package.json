{
  "name": "mediathek-cli",
  "version": "0.3.2",
  "description": "query databases of german(-speaking) publicly funded TV-Stations (OER) via this simple CLI tool.",
  "author": "Max @maxboettinger",
  "bin": {
    "media": "./bin/run"
  },
  "homepage": "https://github.com/maxboettinger/mediathek-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "maxboettinger/mediathek-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "@oclif/plugin-plugins": "^5.4.45",
    "@types/inquirer": "^9.0.7",
    "inquirer": "^9.3.7",
    "axios": "^1.11.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.13",
    "@types/chai": "^5.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "chai": "^5.1.2",
    "eslint": "^9.31.0",
    "eslint-config-oclif": "^6.0.87",
    "globby": "^14.0.2",
    "mocha": "^11.7.1",
    "oclif": "^4.22.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "oclif": {
    "bin": "media",
    "dirname": "media",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "local": "npm run build && clear && ./bin/run query tagesschau"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/maxboettinger/mediathek-clik/issues",
  "keywords": [
    "mediathek",
    "OER",
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
